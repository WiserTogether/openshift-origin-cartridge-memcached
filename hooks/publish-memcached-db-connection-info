#!/bin/bash
memcached_url="memcached://$OPENSHIFT_INTERNAL_IP:$OPENSHIFT_MEMCACHED_PROXY_PORT/"

echo "OPENSHIFT_MEMCACHED_GEAR_UUID=$OPENSHIFT_GEAR_UUID"
echo "OPENSHIFT_MEMCACHED_GEAR_DNS=$OPENSHIFT_GEAR_DNS"
echo "OPENSHIFT_MEMCACHED_HOST=$OPENSHIFT_INTERNAL_IP"
echo "OPENSHIFT_MEMCACHED_PORT=$OPENSHIFT_MEMCACHED_PROXY_PORT"
echo "OPENSHIFT_MEMCACHED_URL=$memcached_url"
